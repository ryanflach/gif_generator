<%= form_for(@gif, url: admin_gifs_path) do |f| %>
  <p>
    Enter new category (one word):
  </p>
  <%= f.label :category %>
  <%= f.text_field :category %>
  <p>
    --OR select from existing--
  </p>
  <%= f.label :category_id %>
  <%= f.select :category_id, categories, include_blank: true %><br />

  <%= f.submit "Generate GIF" %>
<% end %>

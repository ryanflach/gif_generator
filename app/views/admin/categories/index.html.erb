<h1>All Categories</h1>
<ul>
  <% Category.all.each do |cat| %>
    <li><%= link_to "#{cat.name} (#{cat.gifs.count} GIFs)", admin_category_path(cat) %></li>
  <% end %>
</ul>
<p>
  <%= link_to "Create Category", new_admin_category_path %>
</p>
<p>
  <%= link_to "Add GIF", new_admin_gif_path %>
</p>
